func remove_specific_lines(text: String) -> String:
    var lines = text.split("\n")
    var filtered_lines = []
    
    var pattern_exclude = RegEx.new()

    pattern_exclude.compile("^(\\|-|![^!]*$)")

    for line in lines:
        line = line.replace("|Start|", "").replace("|End|", "")

        if pattern_exclude.search(line):
            continue

        filtered_lines.append(line)

    return "\n".join(filtered_lines)


func remove_thumb_lines(text: String) -> String:
    var lines = text.split("\n")
    var output = []
    
    var numberpx_regex = RegEx.new()
    numberpx_regex.compile(r"\d+px")
    
    for line in lines:
        var lower_line = line.to_lower()
        if lower_line.find("thumb|") == -1 and lower_line.find("|thumb") == -1 and numberpx_regex.search(line) == null:
            output.append(line)
    
    return "\n".join(output)

func format_date_from_template(date: String) -> String:
    var regex = RegEx.new()
    regex.compile(r"\{\{date\|(\d{4})-(\d{2})-(\d{2})\}\}")

    var result = regex.search_all(date)
    var formatted_date = date

    var month_names = [
        "January", "February", "March", "April", "May", "June", 
        "July", "August", "September", "October", "November", "December"
    ]
    
    for match in result:
        var year = match.get_string(1)
        var month = int(match.get_string(2)) - 1
        var day = match.get_string(3)
        
        var formatted = month_names[month] + " " + str(day) + ", " + year
        
        formatted_date = formatted_date.replace(match.get_string(0), formatted)
    
    return formatted_date
    
func format_time_from_template(hour: String) -> String:
    var regex = RegEx.new()
    regex.compile(r"(?i)\{\{time\|(\d{4}-\d{2}-\d{2})\s+([^}]+)\}\}")
    
    var result = regex.search_all(hour)
    var formatted_hour = hour

    for match in result:
        var time_value = match.get_string(2)
        formatted_hour = formatted_hour.replace(match.get_string(0), time_value)
    
    return formatted_hour

