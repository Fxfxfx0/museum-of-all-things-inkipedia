func remove_nested_templates_and_tables(text: String) -> String:
    var result = ""
    var template_stack = []
    var table_stack = []
    var i = 0

    while i < text.length():
        if text.substr(i, 2) == "{{":
            # Start of a new template, push to stack
            template_stack.append(i)
            i += 2  # Skip past "{{"
        elif text.substr(i, 2) == "}}" and template_stack.size() > 0:
            # End of a template, pop from stack
            template_stack.pop_back()
            i += 2  # Skip past "}}"
        elif text.substr(i, 2) == "{|" and table_stack.size() == 0:
            # Start of a table, push to stack
            table_stack.append(i)
            i += 2  # Skip past "{|"
        elif text.substr(i, 2) == "|}" and table_stack.size() > 0:
            # End of a table, pop from stack
            table_stack.pop_back()
            i += 2  # Skip past "|}"
        elif table_stack.size() == 0 and template_stack.size() == 0:
            # If not inside a table or template, add character to result
            result += text[i]
            i += 1
        else:
            # Inside a template or table, skip characters
            i += 1

    return result

func remove_file_references(text: String) -> String:
    # This regex looks for an optional "File:" or "Image:" prefix,
    # then a word (or any non-space character) ending in a common image extension.
    var file_regex = RegEx.new()
    file_regex.compile("(?i)(File:|Image:)?\\b[^\\s\\|]+\\.(png|jpg|jpeg|gif|webp|svg)\\b")
    return file_regex.sub(text, "", true)

