    var lower_section = section_name.to_lower().strip_edges()
    for ignore in ignore_sections:
        if lower_section.find(ignore.to_lower()) != -1:
            return true
    return false

func trim_equals(s: String) -> String:
    var start = 0
    var end = s.length() - 1
    while start < s.length() and s[start] == "=":
        start += 1
    while end >= 0 and s[end] == "=":
        end -= 1
    if start <= end:
        return s.substr(start, end - start + 1).strip_edges()
    return ""

func remove_curly_braces_and_equal_pipe_lines(text: String) -> String:
    var lines = text.split("\n")
    var output = []
    
    for line in lines:
        if line.find("{") == -1 and line.find("}") == -1 and not (line.find("|") != -1 and line.find("=") != -1):
            output.append(line)
    
    return "\n".join(output)
    
