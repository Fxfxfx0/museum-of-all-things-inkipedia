func process_match(input_text: String) -> String:
    var regex = RegEx.new()
    regex.compile("\\{\\{(OC|OE|OV|S|S2|S2OE|S3|S3EP|S3SO|SO|SR|Ss|SRNW|RotM|SSBU|'s|MK8D|EU|EUOC|JP|NA|NAEUOC|NAOC|NLB)\\}\\}")
    
    var matches = regex.search_all(input_text)
    var output = input_text
    
    for match in matches:
        var matched_str = match.strings[0]
        var match_key = match.strings[1]
        
        if match_dict.has(match_key):
            output = output.replace(matched_str, match_dict[match_key])
            
    return output

