func remove_ignored_sections(text: String) -> String:
    var lines = text.split("\n")
    var output = []
    var ignore_stack = []

    for line in lines:
        if line.begins_with("="):
            var header_level = 0
            for i in range(line.length()):
                if line[i] == "=":
                    header_level += 1
                else:
                    break
            var header_text = trim_equals(line.strip_edges())

            if should_ignore_section(header_text):
                ignore_stack.append(header_level)
                continue
            else:
                while ignore_stack.size() > 0 and header_level <= ignore_stack[ignore_stack.size() - 1]:
                    ignore_stack.pop_back()
                if ignore_stack.size() == 0:
                    output.append(line)
                continue
        else:
            if ignore_stack.size() == 0:
                output.append(line)
    return "\n".join(output)

