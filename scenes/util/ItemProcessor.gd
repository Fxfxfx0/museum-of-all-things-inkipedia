        "__END__", "__INDEX__", "__NOINDEX__", "__STATICREDIRECT__", "__EXPECTUNUSEDTEMPLATE__"
    ]
    
    for directive in directives:
        text = text.replace(directive, "")
    
    return text.strip_edges()

